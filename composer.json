{
  "name": "google/photos-library",
  "type": "library",
  "description": "Google Photos Library API Client Library for PHP",
  "keywords": ["google","google photos"],
  "homepage": "https://developers.google.com/photos",
  "license": "Apache-2.0",
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Google\\Photos\\Library\\V1\\": "src/Google/Photos/Library/V1/",
      "Google\\Photos\\Types\\": "src/Google/Photos/Types/",
      "GPBMetadata\\Google\\Photos\\Library\\V1\\": "src/GPBMetadata/Google/Photos/Library/V1/",
      "GPBMetadata\\Google\\Photos\\Types\\": "src/GPBMetadata/Google/Photos/Types/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Google\\Photos\\Library\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^5.5 || ^7.0",
    "ext-bcmath": "*",
    "google/gax": "^1.0.2",
    "google/protobuf": "^3.7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^5.0"
  }
}
